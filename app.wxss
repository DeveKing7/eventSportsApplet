/**app.wxss**/
@import 'style/weui.wxss';
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.primary-color {
    color: rgb(43, 120, 183);
}

.primary-color-background {
    background-color: rgb(43, 120, 183);
}

.flex-row {
    display: flex;
    flex-direction: row;
}

button::after {
    border: none;
}

@font-face {
    font-family: 'HYk2gj';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.authorization{
 
 width: 100%;
 height: 100%;
 position: absolute;
 z-index: 999999;
 top: 0rpx;
 left: 0rpx;
 display: flex;
 flex-direction: column;
 justify-content: center;
 align-items: center;
 align-content: center;
 
 text-align: center;
 background-color: white;
 color: #333;
 font-size: 25rpx;

 



}
.authorization image{

margin-bottom: 40rpx;
width: 154rpx;
height: 197rpx;

}
.authorization button{

margin-top: 40rpx;
width: 221rpx;
height: 70rpx;
color: white;
font-size: 30rpx;
background-color: #0f6baf;
line-height: 70rpx;


}
.WLT_JOIN{

 background-color: rgb(232, 77, 85);


}


 
